{
  "name": "gatsby-starter-theme-workspace",
  "private": true,
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn workspace patternfly-org-demo build",
    "clean": "yarn workspace patternfly-org-demo clean",
    "clean:next": "yarn workspace @patternfly/patternfly clean",
    "develop:next": "yarn workspace @patternfly/patternfly dev -p 8001",
    "clean:react": "yarn workspace @patternfly/react-docs clean",
    "develop:react": "yarn workspace @patternfly/react-docs develop -p 8002"
  },
  "workspaces": {
    "packages": [
      "gatsby-theme-patternfly-org",
      "patternfly-org-demo",
      "patternfly-org-demo/patternfly-next",
      "patternfly-org-demo/patternfly-react/packages/patternfly-4/react-docs",
      "patternfly-org-demo/patternfly-react/packages/patternfly-4/react-docs/plugins/gatsby-transformer-react-docgen-typescript"
    ],
    "nohoist": [
      "**/gatsby-theme-patternfly-org"
    ]
  }
}
 